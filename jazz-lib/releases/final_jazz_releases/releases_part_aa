on Clifford</name><anv></anv><join></join><role>Drums</role><tracks></tracks></artist><artist><id>1241111</id><name>Rob Statham</name><anv></anv><join></join><role>Electric Bass</role><tracks></tracks></artist><artist><id>4951401</id><name>Geoff Williams (4)</name><anv>Jeff Williams</anv><join></join><role>Piano</role><tracks></tracks></artist><artist><id>251235</id><name>Ed Jones</name><anv></anv><join></join><role>Tenor Saxophone, Soprano Saxophone, Producer</role><tracks></tracks></artist><artist><id>251235</id><name>Ed Jones</name><anv>Jones</anv><join></join><role>Written-By</role><tracks></tracks></artist></extraartists></track><track><position>3</position><title>Bolivia</title><duration>8:14</duration><artists><artist><id>27533</id><name>The Ulf Sandberg Quartet</name><anv></anv><join></join><role></role><tracks></tracks></artist></artists><extraartists><artist><id>500113</id><name>Patrik Boman</name><anv></anv><join></join><role>Bass</role><tracks></tracks></artist><artist><id>1199529</id><name>Kurt Roxström</name><anv>Kurt Roxstrom</anv><join></join><role>Drums</role><tracks></tracks></artist><artist><id>777845</id><name>Ulf Sandberg</name><anv></anv><join></join><role>Piano, Producer</role><tracks></tracks></artist><artist><id>430389</id><name>Krister Andersson</name><anv></anv><join></join><role>Tenor Saxophone</role><tracks></tracks></artist><artist><id>135873</id><name>Cedar Walton</name><anv>Walton</anv><join></join><role>Written-By</role><tracks></tracks></artist></extraartists></track><track><position>4</position><title>Old Hat, New Hat</title><duration>5:26</duration><artists><artist><id>27534</id><name>The Beaujolais Band</name><anv></anv><join></join><role></role><tracks></tracks></artist></artists><extraartists><artist><id>270607</id><name>Simon Edwards</name><anv></anv><join></join><role>Bass, Coro</role><tracks></tracks></artist><artist><id>357317</id><name>Dave Pattman</name><anv></anv><join></join><role>Bongos, Percussion, Coro</role><tracks>